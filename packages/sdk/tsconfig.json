{
    "compilerOptions": {
        "module": "ESNext", // or "CommonJS" based on your setup
        "target": "ESNext",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "dist",
        "strict": true,
        "declaration": true, 
        "moduleResolution": "node" // 添加此行
    },
    "include": ["src/**/*"]
